<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".GameActivity"
		android:background="#3F4A4E">

	<Button
			android:text="Run"
			android:layout_width="191dp"
			android:layout_height="94dp"
			android:id="@+id/runButton"
			android:onClick="runButtonClicked"
			android:backgroundTint="#8BC34A"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			android:fontFamily="sans-serif-condensed"
			app:layout_constraintHorizontal_bias="0.502"
			app:layout_constraintTop_toBottomOf="@+id/buttons3x3"
			android:textSize="40sp"
			/>

	<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="325dp"
			android:layout_height="325dp"
			android:id="@+id/buttons3x3"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintVertical_bias="0.333"
			app:layout_constraintHorizontal_bias="0.514">

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button1"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button2"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintEnd_toEndOf="@+id/button3"
				app:layout_constraintStart_toStartOf="@+id/button1"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button3"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button4"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button5"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="@+id/button6"
				app:layout_constraintStart_toStartOf="@+id/button4"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button6"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button7"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button8"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="@+id/button9"
				app:layout_constraintStart_toStartOf="@+id/button7"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>

		<Button
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:id="@+id/button9"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				android:onClick="buttonPanelClick"
				android:backgroundTint="#FFFFFF"
				android:insetBottom="0dp"
				android:insetTop="0dp"
				android:textAllCaps="false"/>
	</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
